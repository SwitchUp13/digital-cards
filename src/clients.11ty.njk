---
pagination:
  data: collections.clients
  size: 1
  alias: client
permalink: "clients/{{ client.fileSlug }}/index.html"
layout: layout.njk
---

<h1>{{ client.businessName }}</h1>
<p>{{ client.about }}</p>

<p>Contact: <a href="mailto:{{ client.contact.email }}">{{ client.contact.email }}</a></p>

<p>LinkedIn: 
  <a href="https://linkedin.com/in/{{ client.contact.linkedin }}" target="_blank">
    {{ client.contact.linkedin }}
  </a>
</p>
